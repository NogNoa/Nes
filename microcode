Mnemonic    addressing  argument-bytes  cycles
[
    operation   src '->' dest
    // load next instruction is implied: read-prog ' ' opcode
]

CLf Impl    0   2
[
    move    0   f
]

SEf Impl    0   2
[
    move    1   f
]

LDr Impl    1   2
[
    read-prog ' '   r
]

CMr Impl    1   2
[
    read-prog ' '   O
    sub     O   r
]

LDr Dir     1   3
[
    read-prog ' '   AD
    move    M   r
]